{
  "manifest_version": 3,
  "name": "URL Blocker - Productivity App",
  "version": "0.0.1",
  "permissions": ["storage", "tabs"],
  "background": {
      "service_worker": "./scripts/background.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
      "default_popup": "popup.html"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["./scripts/content.js"]
      }
  ]
}
